<h1>Add a new category</h1>

<%= form_with(model: @category) do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :icon %>
    <%= form.text_field :icon %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>


<div>
  <%= link_to "Back to categories", categories_path %>
</div>
